{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "PUT",
        "POST",
        "DELETE",
        "HEAD",
        "CONNECT",
        "OPTIONS",
        "TRACE",
        "PATCH"
      ],
      "headers": {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }
    }
  ]
}
